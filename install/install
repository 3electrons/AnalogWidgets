#/bin/sh 

mkdir ../../usr 
mkdir ../../usr/include 
mkdir ../../usr/include/analogwidgets 
mkdir ../../usr/lib 

cp -r ../analogwidgets/analogwidgets/* ../../usr/include/analogwidgets 
cp ../analogwidgets/libanalogwidgets.a ../../usr/lib 
cp ../analogwidgets/libanalogwidgets_plugin.so ../../usr/lib 
